{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build main for win-gui",
            "command": "g++.exe",
            "args": [
                "-static-libstdc++",
                "-static-libgcc",
                "-n",
                "-std=c++20",
                "-s",
                "-mwindows",
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}\\win-gui\\main.cpp",
                "-o",
                "${workspaceFolder}\\win-gui\\build\\win-gui.exe"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
            ],
    "version": "2.0.0"
}